{
  "name": "troubleshooting-panel-console-plugin",
  "version": "0.4.0",
  "private": true,
  "repository": "git@github.com:openshift/troubleshooting-panel-console-plugin",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run ts-node node_modules/.bin/webpack",
    "build-dev": "npm run clean && npm run ts-node node_modules/.bin/webpack",
    "start": "npm run ts-node node_modules/.bin/webpack serve",
    "start-console": "../scripts/start-console.sh",
    "start-forward": "../scripts/start-forward.sh",
    "i18n": "i18n-scripts/build-i18n.sh && node i18n-scripts/set-english-defaults.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "lint": "eslint ./src ./integration-tests --fix &&  stylelint \"src/**/*.css\" --allow-empty-input --fix",
    "lint:tsc": "tsc --noEmit",
    "test:e2e": "cd ./integration-tests && cypress open --env openshift=true",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch",
    "test:unit:coverage": "jest --config jest.config.js --coverage"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^7.0.1",
    "@openshift-console/dynamic-plugin-sdk": "^4.19.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "^4.19.0",
    "@patternfly/react-core": "^5.1.1",
    "@patternfly/react-icons": "^5.1.1",
    "@patternfly/react-styles": "^5.1.1",
    "@patternfly/react-topology": "^6.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.37",
    "@types/react-helmet": "^6.1.4",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.7.1",
    "cypress": "^14.5.4",
    "cypress-multi-reporters": "^1.6.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "i18next-parser": "^9.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "openapi-typescript-codegen": "^0.29.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.9",
    "style-loader": "^3.3.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^5.2.2"
  },
  "consolePlugin": {
    "name": "troubleshooting-panel-console-plugin",
    "version": "0.0.1",
    "displayName": "OpenShift Console Troubleshooting Panel Plugin",
    "description": "Troubleshooting Panel Plugin for OpenShift console. Used to display Korrel8r information",
    "exposedModules": {
      "TroubleshootingPanelActionsProvider": "./hooks/useTroubleshootingPanel",
      "TPReducer": "./redux-reducers",
      "NullContextProvider": "./components/NullContextProvider",
      "usePopover": "./hooks/usePopover"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "dependencies": {
    "react-router-dom-v5-compat": "^6.30.0"
  }
}
